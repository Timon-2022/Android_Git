<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/c"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".enroll.Enrollpage">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="262dp"
        android:layout_height="37dp"
        android:text="Copyright © 2020-2022 志祥的菜鸟app \n     Timon.com All Rights Reserved"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.959" />

    <TextView
        android:id="@+id/textViewCreate"
        android:layout_width="276dp"
        android:layout_height="59dp"
        android:autoSizeTextType="uniform"
        android:text="创建你的账号！"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="88dp" />

    <Button
        android:id="@+id/buttonCreate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="创建"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.761" />

    <EditText
        android:id="@+id/editTextTextNewName"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.639"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.314" />

    <EditText
        android:id="@+id/editTextNewPassword"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.639"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextNewName"
        app:layout_constraintVertical_bias="0.033" />

    <EditText
        android:id="@+id/editTextNewPasswordRe"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.639"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextNewPassword"
        app:layout_constraintVertical_bias="0.033" />

    <TextView
        android:id="@+id/textViewNewName"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:autoSizeTextType="uniform"
        android:text="用户名"
        app:layout_constraintBottom_toBottomOf="@+id/editTextTextNewName"
        app:layout_constraintEnd_toStartOf="@+id/editTextTextNewName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/editTextTextNewName" />

    <TextView
        android:id="@+id/textViewNewPass"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:autoSizeTextType="uniform"
        android:text="输入密码"
        app:layout_constraintBottom_toBottomOf="@+id/editTextNewPassword"
        app:layout_constraintEnd_toStartOf="@+id/editTextNewPassword"
        app:layout_constraintHorizontal_bias="0.622"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/editTextNewPassword"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textViewUseless"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:autoSizeTextType="uniform"
        android:text="重复密码"
        app:layout_constraintBottom_toBottomOf="@+id/editTextNewPasswordRe"
        app:layout_constraintEnd_toStartOf="@+id/editTextNewPasswordRe"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/editTextNewPasswordRe" />
</androidx.constraintlayout.widget.ConstraintLayout>